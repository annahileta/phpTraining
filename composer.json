{
    "name": "auto/mvc",
    "description": "Loads all classes and files needed for MVC framework",
    "autoload": {
        "psr-4": {
            "App\\Components\\": "components/",
            "App\\Config\\": "config/",
            "App\\Controllers\\": "controllers/",
            "App\\Factories\\": "factories/",
            "App\\Interfaces\\": "interfaces/",
            "App\\Models\\": "models/"
        },
        "files": [
            "/MVCFramework.php"
        ]
    },
    "config": {
        "optimize-autoloader":true
    }
}